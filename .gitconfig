[user]
	email = robert.fajta@prezi.com
[color]
	ui = true
[alias]
	a = add
	co = checkout
	c = commit
	d = --no-pager diff
	s = !~/bin/gitstatus.sh
	com = !~/bin/gitcheckout.sh -m
	am = !~/bin/gitadd.sh -m
	au = !~/bin/gitadd.sh -u
	ds = !~/bin/gitdiff.sh -s
	dm = !~/bin/gitdiff.sh -m
	du = !~/bin/gitdiff.sh -u
	da = !~/bin/gitdiff.sh -a
	dws = !~/bin/gitdiff-w.sh -s
	dwm = !~/bin/gitdiff-w.sh -m
	dwu = !~/bin/gitdiff-w.sh -u
	dwa = !~/bin/gitdiff-w.sh -a
	pop = stash pop
	nstash = stash save

[core]
	editor = emacs
	excludesfile = /Users/robertfa/.gitignore
